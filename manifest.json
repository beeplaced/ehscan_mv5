{
    "display_override": ["window-controls-overlay"],
    "protocol_handlers": [
      {
        "protocol": "mailto",
        "url": "/newEmail?to=%s"
      }
    ],
    "name": "PhotoAnalyzeApp",
    "short_name": "Ehscan App",
    "start_url": "/",
    "display": "standalone",
    "background_color": "#EBEBEF",
    "description": "Analyze photos quickly and efficiently.",
    "lang": "en",
    "dir": "auto",
    "theme_color": "#EBEBEF",
    "orientation": "any",
    "icons": [
      {
        "src": "/logo_ios_white_back_192.png",
        "type": "image/png",
        "sizes": "192x192"
      },
      {
        "src": "/shortcut.svg",
        "type": "image/svg+xml",
        "sizes": "192x192"
      }
    ],
    "screenshots": [
      {
        "src": "https://www.pwabuilder.com/assets/screenshots/screen1.png",
        "sizes": "2880x1800",
        "type": "image/png",
        "description": "A screenshot of the home page"
      }
    ],
    "related_applications": [
      {
        "platform": "windows",
        "url": "https://www.ehscan.com"
      }
    ],
    "prefer_related_applications": false,
    "shortcuts": [
      {
        "name": "Open Main Feature",
        "url": "/main-feature",
        "description": "Quick access to the main feature of the app."
      }
    ]
  ,
    "file_handlers": [
      {
        "action": "The url you would like to open when the user chooses to share content to your app. This URL must have code to do something with the returned file.",
        "accept": {
          "text/*": [".txt"]
        }
      }
    ]
  }